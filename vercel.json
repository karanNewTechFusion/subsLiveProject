{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "status": 308,
      "headers": { "Location": "/login" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
